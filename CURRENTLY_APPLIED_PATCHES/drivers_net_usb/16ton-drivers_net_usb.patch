-	/* should we call subdriver's resume function? */
-	callsub =
-		(intf == ctx->control &&
-		drvstate->subdriver &&
-		drvstate->subdriver->resume);
-
-	if (callsub)
-		ret = drvstate->subdriver->resume(intf);
-	if (ret < 0)
-		goto err;
-	ret = usbnet_resume(intf);
-	if (ret < 0 && callsub)
