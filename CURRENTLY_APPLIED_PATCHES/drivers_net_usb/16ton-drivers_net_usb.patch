-	/* reset PHY */
-	ret = lan78xx_read_reg(dev, PMT_CTL, &buf);
-	buf |= PMT_CTL_PHY_RST_;
-	ret = lan78xx_write_reg(dev, PMT_CTL, buf);
-
-	timeout = jiffies + HZ;
-	do {
-		mdelay(1);
-		ret = lan78xx_read_reg(dev, PMT_CTL, &buf);
-		if (time_after(jiffies, timeout)) {
-			netdev_warn(dev->net, "timeout waiting for PHY Reset");
-			return -EIO;
