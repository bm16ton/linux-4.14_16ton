-			}
-		} while (buf & OTP_STATUS_BUSY_);
-
-		ret = lan78xx_read_reg(dev, OTP_RD_DATA, &buf);
-
-		data[i] = (u8)(buf & 0xFF);
-	}
-
-	return 0;
-}
