-/* verify that the ethernet protocol is IPv4 or IPv6 */
-static bool is_ip_proto(__be16 proto)
-{
-	switch (proto) {
-	case htons(ETH_P_IP):
-	case htons(ETH_P_IPV6):
-		return true;
-	}
-	return false;
