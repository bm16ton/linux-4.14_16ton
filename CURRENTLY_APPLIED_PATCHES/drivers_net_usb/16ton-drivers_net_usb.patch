-		ret = lan78xx_write_reg(dev, DP_DATA, buf[i]);
-
-		ret = lan78xx_write_reg(dev, DP_CMD, DP_CMD_WRITE_);
-
-		ret = lan78xx_dataport_wait_not_busy(dev);
