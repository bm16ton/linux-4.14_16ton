-	netif_stop_queue(netdev);
-	if (netdev->flags & IFF_PROMISC) {
-		rx_creg |= 0x0001;
-		dev_info(&netdev->dev, "%s: promiscuous mode\n", netdev->name);
-	} else if (!netdev_mc_empty(netdev) ||
-		   (netdev->flags & IFF_ALLMULTI)) {
