-		}
-
-		if ((result = kaweth_download_firmware(kaweth,
-						      "kaweth/trigger_code.bin",
-						      126,
-						      2)) < 0) {
-			dev_err(dev, "Error downloading trigger code (%d)\n",
-				result);
-			goto err_fw;
-
-		}
-
-		if ((result = kaweth_download_firmware(kaweth,
-						      "kaweth/trigger_code_fix.bin",
-						      126,
