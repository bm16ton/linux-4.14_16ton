-		   netif_device_present (dev->net) &&
-		   netif_carrier_ok(dev->net) &&
-		   !timer_pending(&dev->delay) &&
-		   !test_bit(EVENT_RX_PAUSED, &dev->flags) &&
-		   !test_bit(EVENT_RX_HALT, &dev->flags)) {
