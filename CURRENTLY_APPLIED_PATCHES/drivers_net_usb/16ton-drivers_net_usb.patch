-static void ax88772_suspend(struct usbnet *dev)
-{
-	struct asix_common_private *priv = dev->driver_priv;
-	u16 medium;
-
-	/* Stop MAC operation */
-	medium = asix_read_medium_status(dev, 1);
-	medium &= ~AX_MEDIUM_RE;
