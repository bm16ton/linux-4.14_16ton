-static int __must_check smsc95xx_wait_eeprom(struct usbnet *dev)
-{
-	unsigned long start_time = jiffies;
-	u32 val;
-	int ret;
-
-	do {
-		ret = smsc95xx_read_reg(dev, E2P_CMD, &val);
-		if (ret < 0) {
-			netdev_warn(dev->net, "Error reading E2P_CMD\n");
