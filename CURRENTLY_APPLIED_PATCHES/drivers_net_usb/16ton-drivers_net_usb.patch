-		   phy_id, loc, val);
-	mutex_lock(&dev->phy_mutex);
-	sr_set_sw_mii(dev);
-	sr_write_cmd(dev, SR_CMD_WRITE_MII_REG, phy_id, (__u16)loc, 2, &res);
-	sr_set_hw_mii(dev);
