-static void sierra_sync_timer(unsigned long syncdata)
-{
-	struct usbnet *dev = (struct usbnet *)syncdata;
-
-	dev_dbg(&dev->udev->dev, "%s", __func__);
-	/* Kick the tasklet */
-	sierra_net_defer_kevent(dev, SIERRA_NET_TIMER_EXPIRY);
