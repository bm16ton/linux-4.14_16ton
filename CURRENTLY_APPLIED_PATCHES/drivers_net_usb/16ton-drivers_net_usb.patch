-	/* Fill in the URB for shipping it out. */
-	usb_fill_bulk_urb(odev->mux_bulk_tx_urb,
-			  odev->parent->usb,
-			  usb_sndbulkpipe(odev->parent->usb,
-					  odev->out_endp->
-					  bEndpointAddress & 0x7F),
-			  odev->mux_bulk_tx_buf, skb->len, write_bulk_callback,
-			  odev);
