-                                           KAWETH_PACKET_FILTER_BROADCAST |
-                                           KAWETH_PACKET_FILTER_MULTICAST);
-
-	if(result < 0) {
-		dev_err(dev, "Error setting receive filter\n");
-		goto err_free_netdev;
