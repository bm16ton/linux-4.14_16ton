-#define INT_ENP_TDFU_			((u32)BIT(13))	/* TX FIFO Underrun */
-#define INT_ENP_TDFO_			((u32)BIT(12))	/* TX FIFO Overrun */
-#define INT_ENP_RXDF_			((u32)BIT(11))	/* RX Dropped Frame */
