-	/* Don't need rfe_ctl_lock during initialisation */
-	ret = lan78xx_read_reg(dev, RFE_CTL, &pdata->rfe_ctl);
-	pdata->rfe_ctl |= RFE_CTL_BCAST_EN_ | RFE_CTL_DA_PERFECT_;
-	ret = lan78xx_write_reg(dev, RFE_CTL, pdata->rfe_ctl);
