-	case HSO_PORT_APP:
-		port_name = "Application";
-		break;
-	case HSO_PORT_APP2:
-		port_name = "Application2";
-		break;
-	case HSO_PORT_GPS:
-		port_name = "GPS";
-		break;
-	case HSO_PORT_GPS_CONTROL:
-		port_name = "GPS Control";
-		break;
-	case HSO_PORT_PCSC:
-		port_name = "PCSC";
-		break;
-	case HSO_PORT_DIAG:
-		port_name = "Diagnostic";
