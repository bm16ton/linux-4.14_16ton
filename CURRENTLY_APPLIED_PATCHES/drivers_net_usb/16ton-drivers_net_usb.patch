-		/* send now if this NDP is full */
-		if (index >= CDC_NCM_DPT_DATAGRAMS_MAX) {
-			ready2send = 1;
-			ctx->tx_reason_ndp_full++;	/* count reason for transmitting */
-			break;
-		}
-	}
