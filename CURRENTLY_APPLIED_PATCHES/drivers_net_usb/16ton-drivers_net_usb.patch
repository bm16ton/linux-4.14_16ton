-		ret = lan78xx_write_reg(dev, OTP_FUNC_CMD, OTP_FUNC_CMD_READ_);
-		ret = lan78xx_write_reg(dev, OTP_CMD_GO, OTP_CMD_GO_GO_);
-
-		timeout = jiffies + HZ;
-		do {
-			udelay(1);
-			ret = lan78xx_read_reg(dev, OTP_STATUS, &buf);
