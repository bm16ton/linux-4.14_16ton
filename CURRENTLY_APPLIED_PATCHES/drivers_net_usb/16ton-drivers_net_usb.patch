-	sr_set_hw_mii(dev);
-	mutex_unlock(&dev->phy_mutex);
-
-	netdev_dbg(dev->net,
-		   "%s : phy_id=0x%02x, loc=0x%02x, returns=0x%04x\n", __func__,
-		   phy_id, loc, le16_to_cpu(res));
