-
-	ret = lan78xx_read_reg(dev, OTP_PWR_DN, &buf);
-
-	if (buf & OTP_PWR_DN_PWRDN_N_) {
-		/* clear it and wait to be cleared */
-		ret = lan78xx_write_reg(dev, OTP_PWR_DN, 0);
-
-		timeout = jiffies + HZ;
-		do {
-			usleep_range(1, 10);
-			ret = lan78xx_read_reg(dev, OTP_PWR_DN, &buf);
-			if (time_after(jiffies, timeout)) {
-				netdev_warn(dev->net,
-					    "timeout on OTP_PWR_DN");
