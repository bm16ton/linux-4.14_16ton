-		urb = usb_alloc_urb(0, flags);
-		if (urb != NULL) {
-			ret = rx_submit(dev, urb, flags);
-			if (ret)
-				goto err;
-		} else {
-			ret = -ENOMEM;
-			goto err;
-		}
-	}
-err:
-	return ret;
