-	ret = lan78xx_read_reg(dev, RX_ADDRH, &addr_hi);
-
-	addr[0] = addr_lo & 0xFF;
-	addr[1] = (addr_lo >> 8) & 0xFF;
-	addr[2] = (addr_lo >> 16) & 0xFF;
-	addr[3] = (addr_lo >> 24) & 0xFF;
-	addr[4] = addr_hi & 0xFF;
-	addr[5] = (addr_hi >> 8) & 0xFF;
