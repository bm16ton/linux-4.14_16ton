-	ctx->tx_curr_frame_num = n;
-
-	if (n == 0) {
-		/* wait for more frames */
-		/* push variables */
-		ctx->tx_curr_skb = skb_out;
-		goto exit_no_skb;
-
-	} else if ((n < ctx->tx_max_datagrams) && (ready2send == 0) && (ctx->timer_interval > 0)) {
-		/* wait for more frames */
-		/* push variables */
-		ctx->tx_curr_skb = skb_out;
-		/* set the pending count */
