-static void sierra_net_defer_kevent(struct usbnet *dev, int work)
-{
-	struct sierra_net_data *priv = sierra_net_get_private(dev);
-
-	set_bit(work, &priv->kevent_flags);
-	schedule_work(&priv->sierra_net_kevent);
-}
