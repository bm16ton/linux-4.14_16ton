-static int lan78xx_wait_eeprom(struct lan78xx_net *dev)
-{
-	unsigned long start_time = jiffies;
-	u32 val;
-	int ret;
-
-	do {
-		ret = lan78xx_read_reg(dev, E2P_CMD, &val);
