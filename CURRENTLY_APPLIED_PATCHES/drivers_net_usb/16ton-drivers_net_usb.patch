-	kaweth->irq_urb = usb_alloc_urb(0, GFP_KERNEL);
-	if (!kaweth->irq_urb)
-		goto err_tx_and_rx;
-
-	kaweth->intbuffer = usb_alloc_coherent(	kaweth->dev,
-						INTBUFFERSIZE,
-						GFP_KERNEL,
