-static void ch9200_status(struct usbnet *dev, struct urb *urb)
-{
-	int link;
-	unsigned char *buf;
-
-	if (urb->actual_length < 16)
-		return;
-
-	buf = urb->transfer_buffer;
-	link = !!(buf[0] & 0x01);
-
-	if (link) {
-		netif_carrier_on(dev->net);
-		usbnet_defer_kevent(dev, EVENT_LINK_RESET);
-	} else {
-		netif_carrier_off(dev->net);
-	}
-}
