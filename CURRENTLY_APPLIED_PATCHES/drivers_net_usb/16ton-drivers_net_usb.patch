-			return -EIO;
-
-		if (!(val & E2P_CMD_EPC_BUSY_) ||
-		    (val & E2P_CMD_EPC_TIMEOUT_))
-			break;
-		usleep_range(40, 100);
-	} while (!time_after(jiffies, start_time + HZ));
-
-	if (val & (E2P_CMD_EPC_TIMEOUT_ | E2P_CMD_EPC_BUSY_)) {
-		netdev_warn(dev->net, "EEPROM read operation timeout");
